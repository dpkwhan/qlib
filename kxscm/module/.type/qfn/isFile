// @author Jaskirat Rajasansir
//
// @kind function
//
// @fileOverview Checks whether the input is a file
//
// @param x {#any} Data to check
//
// @returns {boolean} True if the input is a file reference and the file exists, false 
// otherwise
{
  if[not .type.isFilePath x;'"IllegalArgumentException";];
  :x~key x;
  }
